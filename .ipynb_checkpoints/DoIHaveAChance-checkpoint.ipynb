   "source": [
    "def get_similarity(x,y):\n",
    "    assert len(x) == len(y) , \"x should be same size as y\"\n",
    "    return pearsonr(torch.FloatTensor([x]).to(dev),torch.FloatTensor([y]).to(dev))"
   ]
  },
